# Discord Bot Token (required)
DISCORD_TOKEN=your_bot_token_here

# Default channel ID for scheduling (optional, can be set via commands)
DEFAULT_CHANNEL_ID=your_channel_id_here

# Admin user IDs (comma-separated, optional)
ADMIN_USER_IDS=user_id_1,user_id_2,user_id_3

# Database path (optional, defaults to scheduler.db)
DATABASE_PATH=scheduler.db